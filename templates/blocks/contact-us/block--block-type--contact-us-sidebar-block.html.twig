{% extends "@molecules/02-blocks/05-block-contact-us.twig" %}

{% block title %}
    <h4 class="granjon-heading-text">{{ content.field_block_heading['0']['#context'].value }}</h4>
{% endblock %}

{% block department %}
    <h5>{{ content.field_department_name['0']['#context'].value }}</h5>
{% endblock %}

{% block telephone %}
    {{ content.field_contact_number }}
{% endblock %}

{% block email %}
    {{ content.field_contact_email}}
{% endblock %}

{% block address %}
    {{ content.field_contact_address }}
{% endblock %}